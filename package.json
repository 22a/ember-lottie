{
  "name": "ember-lottie",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:qonto/ember-lottie.git"
  },
  "license": "MIT",
  "author": "",
  "scripts": {
    "build": "pnpm run build --workspace @qonto/ember-lottie",
    "lint": "pnpm run lint --workspaces --if-present",
    "lint:fix": "pnpm run lint:fix --workspaces --if-present",
    "start": "concurrently 'npm:start:*' --restart-after 5000 --prefix-colors cyan,white,yellow",
    "start:addon": "pnpm start --workspace @qonto/ember-lottie -- --no-watch.clearScreen",
    "start:test-app": "pnpm start --workspace test-app",
    "test": "pnpm run test --workspaces --if-present"
  },
  "devDependencies": {
    "concurrently": "^7.2.1",
    "prettier": "^2.5.1"
  },
  "workspaces": [
    "ember-lottie",
    "test-app"
  ],
  "engines": {
    "node": "14.* || 16.* || >= 18"
  }
}
